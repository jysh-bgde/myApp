<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= title %></title>
</head>
<body>
 <%= user.name%>
 <%= user.date_of_birth_formatted%>
 <%= user.country%>
 <%= user.city%>
 <%= user.gender%>

 <% if (list_user_posts==null) { %>
  <h1>NO POSTS</h1>
  <% } else { %>
 <ul>
  <% list_user_posts.forEach(function(post){ %>
    <li><a href= "<%= post.url %>" > <%= post.name %> - </a><%= post.user_id %> -<%= post.details%></li>
    <% }) %>
</ul>
<% } %>

</body>
</html>